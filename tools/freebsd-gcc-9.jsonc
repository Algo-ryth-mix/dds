{
    "$schema": "../res/toolchain-schema.json",
    "compiler_id": "gnu",
    "c_compiler": "gcc9",
    "cxx_compiler": "g++9",
    "flags": [
        "-DSPDLOG_COMPILED_LIB", // Required to use a compiled spdlog
        "-Werror=return-type",
    ],
    "cxx_flags": [
        "-fconcepts",
        "-std=c++2a",
    ],
    "link_flags": [
        "-static-libgcc",
        "-static-libstdc++",
    ],
    // "debug": true,
    "optimize": true,
    "compiler_launcher": "ccache"
}